{
    "categories": [
        {
            "title": "## 🌟 Features",
            "labels": [
                "🚀",
                "🌟"
            ]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": [
                "🐛",
                "🚑"
            ]
        },
        {
            "title": "## 💬 Other",
            "labels": []
        }
    ],
    "pr_template": "${{TITLE}}",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1"
        }
    ],
    "transformers": [
        {
            "pattern": "(\\[ImgBot\\]) (.+)",
            "target": "- [ImgBot] $2"
        },
        {
            "pattern": "(.) (.+)",
            "target": "- $2"
        }
    ],
    "exclude_merge_branches": [
        "merge pull request",
        "Merge pull request"
    ]
}